# Application config
spring:
  application:
    name: product-service
  h2:
    console:
      enabled: true
      path: /h2
  jpa:
    show-sql: true
  datasource:
    url: jdbc:h2:mem:productdb
    username: sa
    password: 
    driver-class-name: org.h2.Driver

# HTTP Server (Tomcat) Port
server: 
  port: 8083

# Discovery Server Access
eureka:
    client:
      serviceUrl:
        defaultZone: http://localhost:1111/eureka/
